{
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "y"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["J"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<C-w>", "m"],
      "commands": ["workbench.action.toggleEditorWidths"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "+", "p"]
    },
    {
      "before": ["<leader>", "y"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": ["<leader>", "n", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":wq"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["J"]
    },
    {
      "before": ["t", "n"],
      "commands": [":tabnext"]
    },
    {
      "before": ["t", "p"],
      "commands": [":tabprev"]
    },
    {
      "before": ["<leader>", "v", "s"],
      "commands": [":vsp"]
    },
    {
      "before": ["<C-w>", "="],
      "commands": [
        { "command": "workbench.action.increaseViewSize", "args": [] }
      ]
    },
    {
      "before": ["<C-w>", "-"],
      "commands": [
        { "command": "workbench.action.decreaseViewSize", "args": [] }
      ]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.gotoSymbol"]
    }
  ],
  "vim.foldfix": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-e>": false,
    "<C-a>": false,
    "<C-f>": false,
    "<C-c>": false,
    "<C-j>": false,
    "<C-b>": false,
    "<C-k>": false,
    "<Esc>": false,
    "<C-h>": false
  },
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    }
  ],
  "vim.surround": true,
  "vim.camelCaseMotion.enable": true,
  "vim.highlightedyank.enable": true,

  "search.followSymlinks": false,
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "italic font",
        "scope": [
          "comment",
          "keyword",
          "keyword.control.import",
          "keyword.control.default",
          "keyword.control.from",
          "keyword.operator.new",
          "keyword.control.export",
          "keyword.control.flow",
          "storage.type.class",
          "variable.language.super",
          "variable.language.this",
          "support.type.primitive",
          "entity.other.attribute-name",
          "punctuation.definition.comment",
          "text.html.basic entity.other.attribute-name.html",
          "text.html.basic entity.other.attribute-name",
          "source.js constant.other.object.key.js string.unquoted.label.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.lineHeight": 40,
  "markdown.preview.fontSize": 16,

  "liveServer.settings.donotShowInfoMsg": true,
  "terminal.integrated.lineHeight": 1.0,
  "workbench.list.horizontalScrolling": true,
  "workbench.editor.showTabs": true,
  "debug.console.fontFamily": "FiraCode Nerd Font",
  "breadcrumbs.enabled": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.limit.enabled": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "breadcrumbs.symbolPath": "off",
  "editor.fontFamily": "'CaskaydiaCove Nerd Font','Operator Mono Medium','hack', 'JetBrains Mono','Source Code Pro', 'Fira Code',  Menlo",
  "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font','FiraCode Nerd Font','JetBrains Mono','Fira Code',Menlo,Monaco, 'Courier New', monospace",
  "liveServer.settings.donotVerifyTags": true,
  "debug.console.fontSize": 16,
  "terminal.integrated.fontSize": 16,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "zenMode.centerLayout": false,
  "zenMode.restore": true,
  "editor.tabSize": 2,
  "files.autoSave": "off",
  "editor.suggestSelection": "first",
  "explorer.confirmDragAndDrop": false,
  "cSpell.userWords": [
    "alipay",
    "autoresize",
    "axios",
    "blueimp",
    "blueimp md",
    "easymotion",
    "echarts",
    "foxmail",
    "hlsearch",
    "incsearch",
    "kase",
    "kruskal",
    "localforage",
    "mayapony",
    "md",
    "nestjs",
    "renda",
    "spfa",
    "stoi",
    "tbody",
    "thead",
    "tsinghua",
    "vite",
    "vuex"
  ],
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "[cpp]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true,
    "editor.tabSize": 4
  },
  "[c]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true,
    "editor.tabSize": 4
  },

  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.iconTheme": "material-icon-theme",
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "files.associations": {
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.html": "html",
    ".css": "css"
  },
  "emmet.includeLanguages": {
    "wxml": "html"
  },
  "extensions.ignoreRecommendations": false,

  "editor.lineNumbers": "relative",

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },

  "editor.quickSuggestions": {
    "strings": true
  },
  "git.enableSmartCommit": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "python.experiments.optOutFrom": ["pythonJediLSP"],
  "terminal.integrated.profiles.linux": {
    "fish": {
      "path": "fish"
    }
  },
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.copyOnSelection": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enabled": true,
  "cSpell.enableFiletypes": ["vue"],

  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}/src"
  },
  "editor.minimap.enabled": false,
  "editor.wordWrapColumn": 120,
  "editor.wordWrap": "on",
  "editor.suggest.preview": false,
  "security.workspace.trust.untrustedFiles": "open",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },

  "editor.quickSuggestionsDelay": 0,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultDateFormat": null,
  "editor.fontSize": 20,
  "editor.hover.delay": 200,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorSurroundingLines": 15,
  "editor.cursorWidth": 3,
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "workbench.editor.historyBasedLanguageDetection": true,
  "editor.hover.sticky": false,
  "emmet.triggerExpansionOnTab": true,
  "window.menuBarVisibility": "toggle",
  "editor.inlineSuggest.enabled": true,

  "window.zoomLevel": 1,
  "workbench.colorTheme": "Dracula"
}
