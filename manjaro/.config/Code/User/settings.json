{
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "a"],
      "commands": ["wwm.aligncode"]
    },
    {
      "before": ["<leader>", "y"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["J"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "a"],
      "commands": ["wwm.aligncode"]
    },
    {
      "before": ["<C-w>", "m"],
      "commands": ["workbench.action.toggleEditorWidths"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "+", "p"]
    },
    {
      "before": ["<leader>", "y"],
      "after": ["\"", "+", "y"]
    },
    {
      "before": ["<leader>", "n", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":wq"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["J"]
    },
    {
      "before": ["t", "n"],
      "commands": [":tabnext"]
    },
    {
      "before": ["t", "p"],
      "commands": [":tabprev"]
    },
    {
      "before": ["<leader>", "v", "s"],
      "commands": [":vsp"]
    },
    {
      "before": ["<C-w>", "="],
      "commands": [{ "command": "workbench.action.increaseViewSize", "args": [] }]
    },
    {
      "before": ["<C-w>", "-"],
      "commands": [{ "command": "workbench.action.decreaseViewSize", "args": [] }]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.gotoSymbol"]
    }
  ],
  "vim.foldfix": true,
